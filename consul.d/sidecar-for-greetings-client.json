{
  "service": {
    "name": "greetings-client",
    "id":"greetings-client-v1", 
    "address": "10.5.0.3",
    "port": 8080,
    "connect": {
      "sidecar_service": {
        "port": 19001,
        "check": {
          "name": "Connect Envoy Sidecar",
          "tcp": "10.5.0.3:19001",
          "interval": "10s"
        },
        "proxy": {
          "upstreams": [{
             "destination_name": "greetings-api",
             "local_bind_address": "127.0.0.1",
             "local_bind_port": 5000
          }]
        }
      }
    }
  }
}